{
    "name": "geotic",
    "version": "4.3.2",
    "description": "entity-component-system",
    "main": "dist/geotic.umd.js",
    "module": "dist/geotic.esm.js",
    "types": "dist/index.d.ts",
    "type": "module",
    "scripts": {
        "start": "npm run build && node --enable-source-maps --experimental-modules --es-module-specifier-resolution=node example/index.js",
        "build": "rollup -c",
        "build:types": "tsc --emitDeclarationOnly",
        "type-check": "tsc --noEmit",
        "pretty": "prettier --write .",
        "benchmark": "node --experimental-modules --es-module-specifier-resolution=node ./benchmark.js",
        "test": "jest --config jest.config.json --runInBand --detectOpenHandles --forceExit",
        "prepublish": "npm run build"
    },
    "author": "Dalton Mills <mills.dalton@gmail.com> (http://www.ddmills.com)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ddmills/geotic/issues"
    },
    "homepage": "https://github.com/ddmills/geotic#readme",
    "dependencies": {
        "camelcase": "8.0.0",
        "deepmerge": "^4.3.1",
        "tslib": "^2.4.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.17.3",
        "@babel/core": "^7.9.0",
        "@rollup/plugin-typescript": "^12.1.3",
        "@babel/preset-env": "^7.9.5",
        "@rollup/plugin-commonjs": "^28.0.0",
        "@rollup/plugin-node-resolve": "^16.0.1",
        "@types/jest": "^30.0.0",
        "@types/node": "24.0.3",
        "babel-jest": "25.5.0",
        "chance": "1.1.4",
        "jest": "^30.0.1",
        "prettier": "^3.5.3",
        "rollup": "^4.44.0",
        "@rollup/plugin-babel": "^6.0.4",
        "typescript": "^5.8.3"
    },
    "files": [
        "src",
        "dist"
    ],
    "directories": {
        "lib": "src"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ddmills/geotic.git"
    },
    "keywords": [
        "Entity",
        "Component",
        "System",
        "ES6",
        "Class",
        "Javascript",
        "Framework",
        "Game",
        "Engine",
        "ecs",
        "ec",
        "js"
    ]
}
